{
    "name": "tschope/iptv-cms",
    "type": "project",
    "description": "This is a cms control iptv channels.",
    "authors": [
        {
            "name": "Felipe Mateus",
            "email": "eu@felipemateus.com",
            "homepage": "https://felipemateus.com/blog/2021/08/pacote-gerenciador-de-canais-iptv-no-laravel",
            "role": "Developer"
        }
    ],
    "keywords": ["framework", "laravel",
        "Laravel",
        "IPTV",
        "IPTVCHannels",
        "Channels",
        "iptv-channels",
        "channels-m3u8",
        "channels-list",
        "iptv-laravel",
        "m3u8-iptv",
        "channel",
        "Playlist",
        "iptv-playlist",
        "m3u8",
        "laravel-package",
        "customers",
        "iptv-customer",
        "iptv-url",
        "iptv-brasil",
        "canais"
],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1",
        "tschope/iptv-channels": "^4.0",
        "tschope/iptv-core": "^2.0",
        "tschope/iptv-customers": "^0.3",
        "felipemateus/laravel-iptv-gateway-paypal": "^v0.0.3",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.11",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.20",
        "laravel/sail": "^1.16",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.3",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --provider=\"Tschope\\IPTVCore\\IPTVProvider\""
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
